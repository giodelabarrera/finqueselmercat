{% extends 'base.html.twig' %}

{% block body %}
    <h1>StatusNotAvailable</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ statusNotAvailable.id }}</td>
            </tr>
            <tr>
                <th>Datenotavailable</th>
                <td>{% if statusNotAvailable.dateNotAvailable %}{{ statusNotAvailable.dateNotAvailable|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Amountclose</th>
                <td>{{ statusNotAvailable.amountClose }}</td>
            </tr>
            <tr>
                <th>Clientclose</th>
                <td>{{ statusNotAvailable.clientClose }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{% if statusNotAvailable.createdAt %}{{ statusNotAvailable.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{% if statusNotAvailable.updatedAt %}{{ statusNotAvailable.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_status_not_available_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_status_not_available_edit', { 'id': statusNotAvailable.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
