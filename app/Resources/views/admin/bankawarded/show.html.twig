{% extends 'base.html.twig' %}

{% block body %}
    <h1>BankAwarded</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ bankAwarded.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ bankAwarded.name }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ bankAwarded.slug }}</td>
            </tr>
            <tr>
                <th>Numorder</th>
                <td>{{ bankAwarded.numOrder }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{% if bankAwarded.createdAt %}{{ bankAwarded.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{% if bankAwarded.updatedAt %}{{ bankAwarded.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_bankawarded_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_bankawarded_edit', { 'id': bankAwarded.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
